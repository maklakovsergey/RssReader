source 'https://github.com/CocoaPods/Specs.git'
platform :ios, "9.0"

use_frameworks!
inhibit_all_warnings!

def common_pods
	pod 'Alamofire', '~> 4.5.0'
    pod 'SwiftLint', '~> 0.20.1'
    pod 'SWXMLHash', '~> 4.0.0'
    pod 'PureLayout', '~> 3.0.2'
    pod 'Block-KVO', :git => 'https://github.com/Tricertops/Block-KVO.git'
end

target 'RssReader' do
    common_pods
end
